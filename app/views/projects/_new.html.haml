#myModal.modal.fade{"aria-hidden" => "true", "aria-labelledby" => "project-modal", :role => "dialog", :tabindex => "-1"}
  .modal-dialog
    .modal-content
      .modal-header
        %button.close{"aria-label" => "Close", "data-dismiss" => "modal", :type => "button"}
          %span{"aria-hidden" => "true"} &times;
        %h4#myModalLabel.modal-title Новий проект
      .modal-body
        =render 'projects/form', modal: true
      .modal-footer
        %button.btn.btn-default{"data-dismiss" => "modal", :type => "button"} Відмінити
        %button.btn.btn-primary{:type => "button"} Створити